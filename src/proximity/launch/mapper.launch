<launch>
  <include file="$(find turtlebot3_slam)/launch/turtlebot3_slam.launch">
    <arg name="slam_methods" value="cartographer" />
  </include>
  <include file="$(find turtlebot3_teleop)/launch/turtlebot3_teleop_key.launch" launch-prefix="gnome-terminal --command"  output="screen">
  </include>
  <node pkg="proximity" type="robot_position_getter.py" name="robot_position_getter"></node>
  <node pkg="proximity" type="proximity_beacon_tracker.py" name="proximity_beacon_tracker" launch-prefix="gnome-terminal --command" output="screen">
  </node>
</launch>
